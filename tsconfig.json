{
	"compilerOptions": {
		"allowJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"incremental": true,
		"isolatedModules": true,
		"jsx": "preserve",
		"lib": ["esnext"],
		"module": "esnext",
		"moduleResolution": "node",
		"noEmit": true,
		"noImplicitAny": false,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"strict": false,
		"strictNullChecks": false,
		"target": "esnext"
	},
	"include": [],
	"references": [
		{
			"path": "./packages/next"
		},
		{
			"path": "./packages/routes-worker"
		}
	]
}
